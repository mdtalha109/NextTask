
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Board {
  id                    String @id @default(uuid())
  orgId                 String
  title                 String

  createdAt             DateTime @default(now())
  updatedAt             DateTime @updatedAt
}